#!/bin/sh

picom -b --config ~/.config/picom/picom.conf --experimental-backends --backend glx --glx-no-stencil --vsync &
# Other Picom toggles: -b --vsync --no-fading-openclose --refresh-rate 75
# nvidia-settings --config="$XDG_CONFIG_HOME/nvidia-settings-rc" &
# dwmbar &
clipmenud &
sxhkd &
xset r rate 230 50 &
nitrogen --restore &
