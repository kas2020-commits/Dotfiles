super + Return
	$TERMINAL

super + m
	$TERMINAL -e cmus

super + r
	$TERMINAL -e $FILE

super + shift + w
	$BROWSER

super + p
	rofi -show drun

ctrl + alt + l
	$HOME/.config/i3lock/beut.sh

Print
	flameshot full --path ~/Pictures/Screenshots
# 	scrot '%Y%m%d-%T.png' -e 'mv $f ~/Pictures/Screenshots/'

super + Insert
	clipmenu -i -p "Clipboard" -l 20

ctrl + alt + Delete
	power-management

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle; refreshbar

XF86Audio{Raise,Lower}Volume
	pamixer -{i,d} 5; refreshbar

XF86Audio{Play,Pause,Next,Prev}
	playerctl {play-pause,play-pause,next,previous}

XF86MonBrightnessUp
	light -A 10

XF86MonBrightnessDown
	light -U 10
